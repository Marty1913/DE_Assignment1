{
  "steps": [
    {
      "name": "gcr.io/cloud-builders/docker",
      "args": [
        "build",
        "-t",
        "gcr.io/$PROJECT_ID/prediction-ui-embedded-gcs:0.0.1",
        "./Assignment_1/prediction-ui-embedded-gcs"
      ]
    },
    {
      "name": "gcr.io/cloud-builders/docker",
      "args": [
        "push",
        "gcr.io/$PROJECT_ID/prediction-ui-embedded-gcs:0.0.1"
      ]
    },
    {
      "name": "gcr.io/google.com/cloudsdktool/cloud-sdk",
      "entrypoint": "gcloud",
      "args": [
        "run",
        "deploy",
        "prediction-service",
        "--image",
        "gcr.io/$PROJECT_ID/prediction-ui-embedded-gcs:0.0.1",
        "--region",
        "us-central1",
        "--platform",
        "managed",
        "--allow-unauthenticated"
      ]
    }
  ]
}
